{
    "version": 2,
    "builds": [
      {
        "scripts": {
            "build": "./build_files.sh"
          },
        "src": "core/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }

    ],
    "routes": [
      {
        "src": "/staticfiles/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "core/wsgi.py"
      }
    ]
  }